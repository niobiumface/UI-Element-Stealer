{
  "manifest_version": 3,
  "name": "UI Element Stealer",
  "version": "1.0",
  "description": "Save buttons and UI elements with their exact CSS styling",
  "permissions": ["storage", "contextMenus", "activeTab", "scripting", "notifications"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icon48.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icon48.png"
  }
}
